{
  "name": "time-tracker-mcp-server",
  "version": "0.1.0",
  "description": "A Model Context Protocol server",
  "private": true,
  "type": "module",
  "bin": {
    "time-tracker-mcp-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@cloudflare/d1": "^1.4.1",
    "@libsql/client": "^0.15.4",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@types/uuid": "^10.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.42.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "typescript": "^5.3.3"
  }
}
